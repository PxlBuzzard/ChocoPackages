<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>dub</id>
        <version>1.23.0</version>
        <title>dub package manager</title>
        <authors>https://github.com/orgs/dlang/people</authors>
        <owners>Daniel Jost</owners>
        <licenseUrl>https://raw.githubusercontent.com/dlang/dub/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/dlang/dub</projectUrl>
        <iconUrl>https://raw.githubusercontent.com/dlang/dub-registry/master/public/images/logo-small.png</iconUrl>
        <packageSourceUrl>https://chocolatey.org/packages/dub</packageSourceUrl>
